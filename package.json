{
    "name": "@stackone/mcp-connectors",
    "version": "0.1.0",
    "description": "MCP connectors for disco.dev",
    "module": "./dist/index.js",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": "./dist/index.js",
        "./package.json": "./package.json"
    },
    "type": "module",
    "files": ["dist", "README.md", "LICENSE"],
    "scripts": {
        "prepare": "husky",
        "build": "bun -b tsdown",
        "prepublishOnly": "bun run build",
        "test": "bun test",
        "check": "biome check .",
        "check:fix": "biome check --fix .",
        "typecheck": "tsgo --noEmit"
    },
    "dependencies": {
        "@1password/connect": "^1.4.2",
        "@linear/sdk": "^55.0.0",
        "@modelcontextprotocol/sdk": "^1.0.6",
        "@notionhq/client": "^4.0.1",
        "node-html-parser": "^7.0.1",
        "okapibm25": "^1.4.1",
        "openai": "^5.12.1",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.5.3",
        "@types/bun": "^1.2.4",
        "@types/node": "^22.13.5",
        "@typescript/native-preview": "^7.0.0-dev.20250623.1",
        "husky": "^9.1.7",
        "lint-staged": "^15.2.0",
        "msw": "^2.10.4",
        "publint": "^0.3.12",
        "tsdown": "^0.13.0",
        "unplugin-unused": "^0.5.1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/StackOneHQ/mcp-connectors.git"
    },
    "keywords": [
        "mcp",
        "model-context-protocol",
        "connectors",
        "stackone",
        "saas",
        "integrations"
    ],
    "author": "StackOne",
    "license": "Apache-2.0",
    "bugs": "https://github.com/StackOneHQ/mcp-connectors/issues",
    "homepage": "https://github.com/StackOneHQ/mcp-connectors#readme",
    "lint-staged": {
        "*": ["biome check --fix --no-errors-on-unmatched"]
    }
}
